<p style="color: green"><%= notice %></p>

<% content_for :title, "Users" %>

<h1>Users</h1>
<div class="text-pink-500 bg-yellow-200 text-xl">
  TailwindCSS está funcionando correctamente 🎉
</div>
<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>




